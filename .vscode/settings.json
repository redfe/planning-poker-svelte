{
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[svelte]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.probe": [
        "javascript",
        "html",
        "markdown",
        "svelte"
    ],
    "eslint.validate": [
        "svelte"
    ]
}