{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c && rm -rf ./docs/storybook-static && build-storybook -s ./docs && rm -rf ./storybook-static/build && mv storybook-static ./docs/storybook-static",
    "dev": "rollup -c -w",
    "start": "sirv docs",
    "storybook": "start-storybook -p 6006 -s ./docs",
    "build-storybook": "build-storybook -s ./docs"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.3.12",
    "babel-loader": "^8.2.3",
    "rollup": "^2.38.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.44.0",
    "svelte-loader": "^3.1.2"
  },
  "dependencies": {
    "firebase": "^8.2.5",
    "sirv-cli": "^1.0.10",
    "uuid": "^8.3.2"
  }
}
